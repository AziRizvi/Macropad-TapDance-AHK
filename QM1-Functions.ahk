YTManualSearch() 

				{
				
					;; Show An Input Box To Get The Search Query From The User.
		
						SearchInput := InputBox("YouTube Query Search", "YouTube Search:")
						
						;; If The Search Value Is Empty Or The User Presses "Cancel", Close The Input Box.
						
							If (SearchInput.Result = "Cancel" || SearchInput.Value = "") 
							Return
						
						SearchQuery := SearchInput.Value

						;; Open The Search Query In Youtube.
	
							{
							
								Open_With_Brave_Beta("https://www.youtube.com/results?search_query=" searchQuery)
								
							}
	
				}

